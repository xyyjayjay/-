 */
using System;
 
namespace Homework03
{
    class Program
    {
        static void Main(string[] args)
        {
            Citizen c1 = new Citizen();
            c1.Input("610101205012120000", "旺财", "火星", "交大", 2, Gender.female);
            c1.Print();
        }
    }
    class Citizen
