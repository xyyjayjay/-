    public string Gender { get; set; }
     public int Age
     { get{retern;}
       set{age=value>0&&value<130?value:-1} //如果年龄取值大于0小于130，则取value，如果不是，则取-1
     }
     public string BirthPlace { get; set; }
     public string FamilyAddress { get; set; }
     //构造方法
